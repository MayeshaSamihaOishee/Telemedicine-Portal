<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:visibility="visible"
    tools:context="sasthoseba.com.sasthoseba.VideoCalltoDoc">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/subscriber_container"  />

        <FrameLayout
            android:layout_width="100dp"
            android:layout_height="145dp"
            android:id="@+id/publisher_container"
            android:layout_gravity="top|end"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginRight="10dp"
            android:padding="3dp"
            android:background="#ffffff" />

        <at.markushi.ui.CircleButton
            android:layout_height="92.5dp"
            android:layout_width="92.5dp"
            android:layout_marginBottom="25dp"
            app:cb_color="#ff0000"
            android:onClick="end"
            android:src="@drawable/ic_call_end_black_24dp"
            android:layout_gravity="bottom|center"
            />

        <at.markushi.ui.CircleButton
            android:layout_height="92.5dp"
            android:layout_width="92.5dp"
            android:layout_marginRight="15dp"
            android:layout_marginBottom="25dp"
            app:cb_color="#eee9e9"
            android:onClick="mutemethod"
            android:src="@drawable/ic_mic_off_black_24dp"
            android:layout_gravity="bottom|right"
            />

        <at.markushi.ui.CircleButton
            android:layout_height="92.5dp"
            android:layout_width="92.5dp"
            android:layout_marginLeft="15dp"
            android:layout_marginBottom="25dp"
            app:cb_color="#eee9e9"
            android:onClick="swapcameramethod"
            android:src="@drawable/ic_switch_camera_black_24dp"
            android:layout_gravity="bottom|left"
            />
    </FrameLayout>

</FrameLayout>
